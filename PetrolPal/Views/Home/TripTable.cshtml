
@{
    ViewBag.Title = "TripTable";
}

@model PetrolPal.Models.Trip

<h2>TripTable</h2>

<div class="row">
    <div class="col-md-4">
        <p>Select the dates to display data</p>
        @using (Html.BeginForm())
        {
            <div class="form-group">
                <label for="BeginDate">From:</label>
                @Html.TextBoxFor(model => model.startTime, new { type = "date" })
            </div>
            <div class="form-group">
                <label for="EndDate">To:</label>
                @Html.TextBoxFor(model => model.endTime, new { type = "date", })
            </div>

            <button type="submit">Get data</button>
        }
        </div>

        <div>
         
        </div>
</div>